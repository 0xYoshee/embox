package genconfig

configuration conf {
	@Runlevel(0) include embox.arch.arm.cortexa9.bundle
	@Runlevel(0) include embox.kernel.cpu.no_bkl
	@Runlevel(0) include embox.kernel.cpu.no_cpudata
	@Runlevel(0) include embox.kernel.no_irq
	@Runlevel(0) include embox.driver.interrupt.no_interrupts
	@Runlevel(0) include embox.kernel.critical

	@Runlevel(0) include embox.kernel.stack(stack_size=4096)
	@Runlevel(1) include embox.driver.diag.no_diag
	@Runlevel(1) include embox.driver.diag(impl="embox__driver__diag__no_diag")

	include embox.compat.libc.stdio.print(support_floating=0)
}
